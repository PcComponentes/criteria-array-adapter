{
  "name": "pccomponentes/criteria-array-adapter",
  "description": "Implementation of pccomponentes/criteria for Array",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.4 | ^8.0",
    "pccomponentes/criteria": "^0.3"
  },
  "autoload": {
    "psr-4": {
      "PcComponentes\\CriteriaArrayAdapter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PcComponentes\\CriteriaArrayAdapter\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "fzaninotto/faker": "^1.9",
    "pccomponentes/coding-standard": "^1.2"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
